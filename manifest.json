{
  "manifest_version": 2,

  "name": "Fake News Detector",
  "description": "This extension will help a user check the validity of news",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
   "default_icon": "Bad Logo.png",
   "default_popup":  "miightwork.html"
  },
  "permissions": [
   "tabs",
   "notifications" 
   ]
}